.album {
    margin: 228rpx auto 0;
    background-color: rgb(238, 216, 216);
    width: 650rpx;
    height: 650rpx;
    border-radius: 40rpx;
    border: solid 4rpx skyblue;
}

.album>image {
    width: 650rpx;
    height: 650rpx;
    border-radius: 40rpx;
}

.album>video {
    width: 650rpx;
    height: 650rpx;
    border-radius: 40rpx;
}

.domain {
    position: relative;
    margin-top: 200rpx;
}

.domain>view:nth-child(1) {
    width: 240rpx;
    height: 120rpx;
    background-color: #d4237a;
    margin: 0 auto;
    border-radius: 80rpx;
}

.dianjiout {
    background-color: #f8077c !important;
    box-shadow: 0px 0px 10rpx #e98888;
}

.domain>view:nth-child(1)>image {
    width: 80rpx;
    height: 80rpx;
    margin-left: 76rpx;
    margin-top: 16rpx;
}


